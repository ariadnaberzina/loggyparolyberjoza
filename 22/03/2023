#include <iostream>
#include <vector>
using namespace std;
int main()
{
	vector <string> valsts{ "USA", "UK", "China", "India", "Canada", "Australia" };
	vector <string> code{ "+1", "+44", "+86", "+91", "+1", "+61" };
	int i= 0;

	string atbilde;
	cin >> atbilde;
	do {
		if (valsts[i] == atbilde) cout << code[i];
	}
